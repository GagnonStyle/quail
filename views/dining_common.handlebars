<div class="red_box acc_info">
    <table id="quick_summaries">
        {{#each dining_commons}}
            <tr>
                <th colspan=2><a href="/dining_commons?dcid={{this.dcid}}">{{this.name}} Dining Commons</a></th>
            </tr>
            <tr>
                <td>Traffic - {{this.crowdedness}}</td>
                <td>Noise - {{this.noise_level}}</td>
            </tr>
            <tr>
                <td colspan=4>&nbsp;</td>
            </tr>
            <tr>
                <td colspan=4><a href="#">Is this info wrong? click here to update it!</a></td>
            </tr>
        {{/each}}
    </table>
</div>