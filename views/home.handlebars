<div class='col-md-6'>
  <select name='dining_commons' class='left_center'>
    {{#each dining_commons}}
      <option value='{{this.name}}'>{{this.name}}</option>
    {{/each}}
  </select>
</div>

<div class='col-md-6'>
  <select name='food' class='right_center'>
    {{#each foods}}
      <option value='{{this}}'>{{this}}</option>
    {{/each}}
  </select>
</div>

<br>
<br>

<div class='col-md-6'>
  <h3>Quick Info</h3>
  <ul>
    {{#each dining_commons}}
      <li class="quick_summary">
        <div>
            <h4>{{this.name}}</h4>
          <dl>
            <div class='stack_right'>
              <dt>Traffic</dt>
              <dd>{{this.traffic}}</dd>
            </div>
            <div class='stack_right'>
              <dt>Noise Level</dt>
              <dd>{{this.noise}}</dd>
            </div>
          </dl>
        </div>
      </li>
    {{/each}}
  </ul>
</div>

